        \DTLiffirstrow{}{\tabularnewline\hline}%
        \bodyrowtwo{\jsmt@surname, \jsmt@firstname}%
      }%
      \tabularnewline\hline
    \fi%
    \printtabtoks
    \end{longtable}
  \fi
}

